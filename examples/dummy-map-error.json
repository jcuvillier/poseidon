            "kind": "triton",
            "name": "A",
            "input": "@{args}",
            "spec": {
                "image": "poseidon/worker/dummy:latest"
            }
        },
        {
            "kind": "triton",
            "name": "B",
            "input": "#{A}",
            "dependencies": [
                "A"
            ],
            "spec": {
                "image": "poseidon/worker/dummy:latest"
            }
        },
        {
            "kind": "triton",
            "name": "C",
            "input": {
                "delay": "10s",
                "payload": "DONE"
            },
            "dependencies": [
                "B"
            ],
            "spec": {
                "image": "poseidon/worker/dummy:latest"
            }
        }
    ],
    "args": {
        "delay": "5s",
        "parallelism": 2,
        "payload": [
            {
                "delay": "10s",
                "payload": "YOUHOU1"
            },
            {
                "delay": "5s",
                "payload": "YOUHOU2",
                "error": true
            },
            {
                "delay": "8s",
                "payload": "YOUHOU3"
            }
        ]
    }
